{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    // "jsx": "react",
    "strictNullChecks": true,
    // "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@assets/*": ["src/assets/*"],
      "@libs/*": ["src/libs/*"],
      "@translations/*": ["translations/*"],
      "@root/*": ["./*"]
    }
  }  
}
