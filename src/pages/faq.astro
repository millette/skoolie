---
// astro
import { getEntry } from 'astro:content'

// self
import Layout from "@layouts/layout.astro"

const entry = await getEntry('pages', 'faq')

export async function getStaticPaths() {
  return [
    {
      params: {
        slug: entry.slug,
        props: {
          entry
        }
      },
    },
  ]
}


const { Content, headings } = await entry.render()
---

<Layout url="/faq/" title={`Skoolie Ã  vendre - ${headings[0].text}`}>

<div class="prose">
<Content />
</div>
</Layout>
