---
// astro
import { getEntry } from 'astro:content'

// self
import Search from "!solid/search.tsx"
import Layout from "!layouts/layout.astro"
import { marketplace } from "!libs/constants.js"

const entry = await getEntry('pages', 'faq-fr')

export async function getStaticPaths() {
  return [
    {
      params: {
        slug: entry.slug,
        props: {
          entry
        }
      },
    },
  ]
}


const { Content, headings } = await entry.render()
---

<Layout url="/faq/" title={`Skoolie Ã  vendre - ${headings[0].text}`}>
  <div class="prose mx-auto">
    <Search client:load lang="fr" />
    <Content marketplace={marketplace} />
  </div>
</Layout>
